item "Run AD Collector"{

    local('$bid');
        foreach $bid ($1){
            runadcollector($bid);
      }
   }  #End Item 


item "Run SET Command"{

    local('$bid');
        foreach $bid ($1){
            runsetcommand($bid);
      }
   }  #End Item 


